{
  "name": "@itwin/presentation-components",
  "version": "5.12.15-alpha.0",
  "description": "React components based on iTwin.js Presentation library",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/iTwin/presentation.git",
    "directory": "packages/components"
  },
  "keywords": [
    "Bentley",
    "EC",
    "Presentation",
    "iTwin",
    "iTwin.js",
    "Frontend",
    "React",
    "Component"
  ],
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "type": "module",
  "main": "lib/cjs/presentation-components.js",
  "types": "lib/cjs/presentation-components",
  "module": "lib/esm/presentation-components.js",
  "exports": {
    ".": {
      "import": "./lib/esm/presentation-components.js",
      "require": "./lib/cjs/presentation-components.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "npm run -s copy:locale && npm run -s build:cjs && npm run -s build:esm",
    "build:cjs": "node ../../scripts/package-cjs.mjs ./lib/cjs && npm run -s copy:cjs && tsc -p tsconfig.cjs.json",
    "build:esm": "npm run -s copy:esm && tsc -p tsconfig.esm.json",
    "copy:locale": "cpx \"./public/**/*\" ./lib/public",
    "copy:cjs": "cpx \"./src/**/*.{scss,json}\" ./lib/cjs",
    "copy:esm": "cpx \"./src/**/*.{scss,json}\" ./lib/esm",
    "clean": "rimraf lib build",
    "cover": "c8 npm run -s test",
    "lint": "eslint ./src/**/*.{ts,tsx}",
    "test:dev": "node --experimental-test-module-mocks --enable-source-maps --import presentation-test-utilities/node-hooks/ignore-styles ./node_modules/mocha/bin/mocha.js --config ./.mocharc.json",
    "test": "npm run test:dev -- --parallel --jobs=4",
    "docs": "npm run -s docs:extract && npm run -s docs:reference && npm run -s docs:changelog",
    "docs:changelog": "cpx ./CHANGELOG.md ./build/docs/reference/presentation-components",
    "docs:reference": "cross-env NODE_PROJECT_ROOT_DIRECTORY=../../ betools docs --json=./build/docs/reference/presentation-components/file.json --tsIndexFile=presentation-components.ts --onlyJson",
    "docs:extract": "betools extract --fileExt=ts,tsx --extractFrom=./src/test --recursive --out=./build/docs/extract",
    "extract-api": "betools extract-api --entry=lib/esm/presentation-components --apiReportFolder=./api --apiReportTempFolder=./api/temp --apiSummaryFolder=./api --includeUnexportedApis",
    "check-internal": "node ../../scripts/checkInternal.js --apiSummary ./api/presentation-components.api.md"
  },
  "dependencies": {
    "@itwin/itwinui-icons-react": "catalog:itwinui",
    "@itwin/itwinui-illustrations-react": "catalog:itwinui",
    "@itwin/presentation-core-interop": "workspace:^",
    "@itwin/presentation-shared": "workspace:^",
    "@itwin/unified-selection": "workspace:^",
    "classnames": "catalog:react",
    "fast-deep-equal": "^3.1.3",
    "fast-sort": "^3.4.1",
    "micro-memoize": "^4.2.0",
    "react-error-boundary": "catalog:react",
    "rxjs": "catalog:rxjs"
  },
  "peerDependencies": {
    "@itwin/appui-abstract": "^4.4.0 || ^5.0.0",
    "@itwin/components-react": "^4.9.0 || ^5.0.0",
    "@itwin/core-bentley": "^4.4.0 || ^5.0.0",
    "@itwin/core-common": "^4.4.0 || ^5.0.0",
    "@itwin/core-frontend": "^4.4.0 || ^5.0.0",
    "@itwin/core-quantity": "^4.4.0 || ^5.0.0",
    "@itwin/core-react": "^4.9.0 || ^5.0.0",
    "@itwin/ecschema-metadata": "^4.4.0 || ^5.0.0",
    "@itwin/imodel-components-react": "^4.9.0 || ^5.0.0",
    "@itwin/itwinui-react": "^3.0.0",
    "@itwin/presentation-common": "^4.4.0 || ^5.0.0",
    "@itwin/presentation-frontend": "^4.4.0 || ^5.0.0",
    "@itwin/unified-selection-react": "^1.0.0",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "@itwin/unified-selection-react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@itwin/appui-abstract": "catalog:itwinjs-core-dev",
    "@itwin/build-tools": "catalog:build-tools",
    "@itwin/components-react": "catalog:appui",
    "@itwin/core-bentley": "catalog:itwinjs-core-dev",
    "@itwin/core-common": "catalog:itwinjs-core-dev",
    "@itwin/core-frontend": "catalog:itwinjs-core-dev",
    "@itwin/core-quantity": "catalog:itwinjs-core-dev",
    "@itwin/core-react": "catalog:appui",
    "@itwin/ecschema-metadata": "catalog:itwinjs-core-dev",
    "@itwin/eslint-plugin": "catalog:build-tools",
    "@itwin/imodel-components-react": "catalog:appui",
    "@itwin/itwinui-react": "catalog:itwinui",
    "@itwin/presentation-common": "catalog:itwinjs-core-dev",
    "@itwin/presentation-frontend": "catalog:itwinjs-core-dev",
    "@itwin/unified-selection-react": "workspace:^",
    "@testing-library/dom": "catalog:test-tools",
    "@testing-library/react": "catalog:test-tools",
    "@testing-library/user-event": "catalog:test-tools",
    "@types/chai": "catalog:test-tools",
    "@types/chai-as-promised": "catalog:test-tools",
    "@types/chai-jest-snapshot": "catalog:test-tools",
    "@types/jsdom": "catalog:test-tools",
    "@types/mocha": "catalog:test-tools",
    "@types/node": "catalog:build-tools",
    "@types/react": "catalog:react",
    "@types/react-dom": "catalog:react",
    "@types/sinon": "catalog:test-tools",
    "@types/sinon-chai": "catalog:test-tools",
    "c8": "catalog:test-tools",
    "chai": "catalog:test-tools",
    "chai-as-promised": "catalog:test-tools",
    "chai-jest-snapshot": "catalog:test-tools",
    "cpx2": "catalog:build-tools",
    "cross-env": "catalog:build-tools",
    "eslint": "catalog:build-tools",
    "eslint-plugin-react": "catalog:build-tools",
    "global-jsdom": "catalog:test-tools",
    "ignore-styles": "catalog:test-tools",
    "jsdom": "catalog:test-tools",
    "mocha": "catalog:test-tools",
    "presentation-test-utilities": "workspace:^",
    "react": "catalog:react",
    "react-dom": "catalog:react",
    "rimraf": "catalog:build-tools",
    "sinon": "catalog:test-tools",
    "sinon-chai": "catalog:test-tools",
    "typescript": "catalog:build-tools"
  }
}
