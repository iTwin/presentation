{
  "name": "@itwin/presentation-testing",
  "version": "4.0.0-dev.1",
  "description": "Testing utilities for iTwin.js Presentation library",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/iTwin/presentation/tree/master/packages/testing"
  },
  "keywords": [
    "Bentley",
    "Presentation",
    "iTwin.js",
    "Testing"
  ],
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "main": "lib/cjs/presentation-testing.js",
  "module": "lib/esm/presentation-testing.js",
  "types": "lib/cjs/presentation-testing.d.ts",
  "scripts": {
    "build": "npm run -s build:cjs && npm run -s build:esm",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:watch": "npm run -s build:cjs -- -w",
    "clean": "rimraf lib",
    "cover": "nyc npm -s test",
    "docs": "npm run -s docs:reference && npm run -s docs:changelog",
    "docs:changelog": "cpx ./CHANGELOG.md ../../build/generated-docs/presentation/presentation-testing",
    "docs:reference": "betools docs --json=../../build/generated-docs/presentation/presentation-testing/file.json --tsIndexFile=presentation-testing.ts --onlyJson",
    "lint": "eslint ./src/**/*.ts",
    "test": "mocha --config ./.mocharc.json \"./lib/cjs/test/**/*.test.js\""
  },
  "dependencies": {
    "@itwin/appui-abstract": "^3.5.0",
    "@itwin/components-react": "^3.5.0",
    "@itwin/core-backend": "^3.5.0",
    "@itwin/core-bentley": "^3.5.0",
    "@itwin/core-common": "^3.5.0",
    "@itwin/core-frontend": "^3.5.0",
    "@itwin/core-geometry": "^3.5.0",
    "@itwin/core-orbitgt": "^3.5.0",
    "@itwin/core-quantity": "^3.5.0",
    "@itwin/core-react": "^3.5.0",
    "@itwin/imodel-components-react": "^3.5.0",
    "@itwin/presentation-backend": "^3.5.0",
    "@itwin/presentation-common": "^3.5.0",
    "@itwin/presentation-components": "^3.5.0",
    "@itwin/presentation-frontend": "^3.5.0",
    "@itwin/webgl-compatibility": "^3.5.0",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@itwin/build-tools": "^3.5.0",
    "@itwin/eslint-plugin": "^3.5.0",
    "@types/chai": "4.3.1",
    "@types/chai-as-promised": "^7",
    "@types/chai-jest-snapshot": "^1.3.0",
    "@types/faker": "^4.1.0",
    "@types/jsdom": "^12.2.0",
    "@types/mocha": "^8.2.2",
    "@types/rimraf": "^2.0.2",
    "@types/sinon": "^9.0.0",
    "@types/sinon-chai": "^3.2.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7",
    "chai-jest-snapshot": "^2.0.0",
    "cross-env": "^5.1.4",
    "cpx2": "^3.0.0",
    "eslint": "^7.11.0",
    "faker": "^4.1.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^19.0.0",
    "jsdom-global": "3.0.2",
    "make-dir": "^1.3.0",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.2.0",
    "typemoq": "^2.1.0",
    "typescript": "~4.4.0"
  }
}
