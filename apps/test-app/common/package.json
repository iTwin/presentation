{
  "name": "@test-app/common",
  "version": "0.0.0",
  "private": true,
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/cjs/index.d.ts",
  "scripts": {
    "build": "npm run -s build:cjs && npm run -s build:esm",
    "build:cjs": "tsc --outDir lib/cjs",
    "build:esm": "tsc --module ESNext --outDir lib/esm",
    "clean": "rimraf lib build",
    "lint": "eslint ./src/**/*.ts",
    "docs": "betools extract --fileExt=ts --extractFrom=./src --recursive --out=./build/docs/extract"
  },
  "dependencies": {
    "@itwin/build-tools": "^4.5.2",
    "@itwin/core-bentley": "^4.5.2",
    "@itwin/core-common": "^4.5.2",
    "@itwin/core-geometry": "^4.5.2",
    "@itwin/core-quantity": "^4.5.2",
    "@itwin/ecschema-metadata": "^4.5.2",
    "@itwin/ecschema-rpcinterface-common": "^4.5.2",
    "@itwin/eslint-plugin": "^4.0.0",
    "@itwin/presentation-common": "^4.5.2",
    "eslint": "^8.57.0",
    "rimraf": "^5.0.5",
    "typescript": "~5.4.5"
  }
}
