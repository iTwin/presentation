{
  "scripts": {
    "start:web": "run-p --silent start:frontend start:web-backend",
    "start:electron": "run-p --silent start:frontend start:electron-backend",
    "start:frontend": "npm run start --prefix ./packages/test-app/frontend",
    "start:web-backend": "npm run start --prefix ./packages/test-app/backend",
    "start:electron-backend": "npm run start:electron --prefix ./packages/test-app/backend",
    "build": "run-p --silent build:*",
    "build:test-app-backend": "npm run build --prefix ./packages/test-app/backend",
    "build:test-app-common": "npm run build --prefix ./packages/test-app/common",
    "lint": "run-p --silent lint:*",
    "lint:test-app-backend": "npm run lint --prefix ./packages/test-app/backend",
    "lint:test-app-common": "npm run lint --prefix ./packages/test-app/common",
    "lint:test-app-frontend": "npm run lint --prefix ./packages/test-app/frontend"
  },
  "engines": {
    "pnpm": ">=6",
    "npm": "<0",
    "node": ">=16"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5"
  }
}
