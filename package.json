{
  "name": "presentation-monorepo",
  "scripts": {
    "start:web": "run-p --silent start:frontend start:web-backend",
    "start:electron": "run-p --silent start:frontend start:electron-backend",
    "start:frontend": "npm run start --prefix ./apps/test-app/frontend",
    "start:web-backend": "npm run start --prefix ./apps/test-app/backend",
    "start:electron-backend": "npm run start:electron --prefix ./apps/test-app/backend",
    "build:all": "run-p build:apps",
    "build:apps": "run-p --silent build:apps:*",
    "build:apps:test-app-backend": "npm run build --prefix ./apps/test-app/backend",
    "build:apps:test-app-common": "npm run build --prefix ./apps/test-app/common",
    "typecheck": "run-p --continue-on-error typecheck:apps",
    "typecheck:apps": "run-p --silent --continue-on-error typecheck:apps:*",
    "typecheck:apps:test-app-backend": "npm run typecheck --prefix ./apps/test-app/backend",
    "typecheck:apps:test-app-common": "npm run typecheck --prefix ./apps/test-app/common",
    "typecheck:apps:test-app-frontend": "npm run typecheck --prefix ./apps/test-app/frontend",
    "lint": "run-p --continue-on-error lint:apps",
    "lint:apps": "run-p --silent --continue-on-error lint:apps:*",
    "lint:apps:test-app-backend": "npm run lint --prefix ./apps/test-app/backend",
    "lint:apps:test-app-common": "npm run lint --prefix ./apps/test-app/common",
    "lint:apps:test-app-frontend": "npm run lint --prefix ./apps/test-app/frontend"
  },
  "engines": {
    "pnpm": ">=6",
    "npm": "<0",
    "node": ">=16"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5"
  }
}
